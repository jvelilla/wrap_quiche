<?xml version="1.0"?>
<ewg_config name="quiche">

  <rule_list>
  	  <!-- Ignore everything that starts with an underscore. -->
    <rule>
      <match>
          <identifier name="_.*"/>  
      </match>
       <wrapper type="none">
      </wrapper>
    </rule>


    	  <!-- Include functions, structs, unions, macros, etc-->
    	  <!-- To learn more read: https://github.com/eiffel-wrap-c/WrapC/blob/master/doc/Readme.md#config_file-->

     
    <rule>
       <match>
         <identifier name="quiche_.*"/>
       </match>
       <wrapper type="default">
       </wrapper>
    </rule> 
  
   <rule>
      <match>
        <type name="callback"/>
      </match>
      <wrapper type="default">
      </wrapper>
    </rule>       
    
    
      <!-- And the rest should be ignored -->
    <rule>
      <match>
      </match>
       <wrapper type="none">
      </wrapper>
    </rule>


  </rule_list>

</ewg_config>